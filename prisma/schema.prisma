// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Category {
  id Int @id @default(autoincrement())
  name String
}

model Product {
  id Int @id @default(autoincrement())
  name String
  priceCents Int
  category Category
}
